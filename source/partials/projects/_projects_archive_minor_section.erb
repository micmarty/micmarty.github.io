<%# Aliases %>
<% projects_archive_minor = data.projects.projects_archive_minor %>

<section class="latest section">
    <div class="section-inner">
        <h2 class="heading">
            <!-- Recent Minor Section Title -->
            <%= projects_archive_minor.section_title %>
        </h2>
        <div class="content">
            <% projects_archive_minor.projects.each do |project| %>
                <%= partial project_partial_path('minor_project_template'), locals: { project: project } %>
            <% end %>
        </div>
    </div>
</section>